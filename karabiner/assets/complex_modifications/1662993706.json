{
  "title": "F2 to cut, F3 to copy, F4 to paste",
  "rules": [{
    "description": "F2 to Cut",
    "manipulators": [{
      "from": {
        "key_code": "f2"
      },
      "to": [{
        "key_code": "x",
        "modifiers": [
          "command"
        ]
      }],
      "type": "basic"
    }]
  }, {
    "description": "F3 to Copy",
    "manipulators": [{
      "from": {
        "key_code": "f3"
      },
      "to": [{
        "key_code": "c",
        "modifiers": ["command"]
      }],
      "type": "basic"
    }]
  }, {
    "description": "F4 to Paste",
    "manipulators": [{
      "from": {
        "key_code": "f4"
      },
      "to": [{
        "key_code": "v",
        "modifiers": ["command"]
      }],
      "type": "basic"
    }]
  }]
}
